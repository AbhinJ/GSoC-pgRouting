#: ../../build/doc/dijkstra-family.rst:249
msgid "Then:"
msgstr ""

#: ../../build/doc/dijkstra-family.rst:258
msgid ""
":math:`\\boldsymbol{\\pi} = \\{(path\\_seq_i, node_i, edge_i, cost_i, "
"agg\\_cost_i)\\}`"
msgstr ""

#: ../../build/doc/dijkstra-family.rst:271
msgid "where:"
msgstr ""

#: ../../build/doc/dijkstra-family.rst:261
msgid ":math:`path\\_seq_i = i`"
msgstr ""

#: ../../build/doc/dijkstra-family.rst:262
msgid ":math:`path\\_seq_{| \\pi |} = | \\pi |`"
msgstr ""

#: ../../build/doc/dijkstra-family.rst:263
msgid ":math:`node_i \\in V`"
msgstr ""

#: ../../build/doc/dijkstra-family.rst:264
msgid ":math:`node_1 = start_{vid}`"
msgstr ""

#: ../../build/doc/dijkstra-family.rst:265
msgid ":math:`node_{| \\pi |}  = end_{vid}`"
msgstr ""

#: ../../build/doc/dijkstra-family.rst:266
msgid ""
":math:`\\forall i \\neq | \\pi |, \\quad (node_i, node_{i+1}, cost_i) "
"\\in E`"
msgstr ""

#: ../../build/doc/dijkstra-family.rst:267
msgid ""
":math:`edge_i  = \\begin{cases}  id_{(node_i, node_{i+1},cost_i)}  "
"&\\quad  \\text{when } i \\neq | \\pi | \\\\ -1 &\\quad  \\text{when } i "
"= | \\pi | \\\\ \\end{cases}`"
msgstr ""

#: ../../build/doc/dijkstra-family.rst:268
msgid ":math:`cost_i = cost_{(node_i, node_{i+1})}`"
msgstr ""

#: ../../build/doc/dijkstra-family.rst:269
msgid ""
":math:`agg\\_cost_i  = \\begin{cases}  0   &\\quad  \\text{when } i = 1  "
"\\\\ \\displaystyle\\sum_{k=1}^{i}  cost_{(node_{k-1}, node_k)}  &\\quad"
"  \\text{when } i \\neq 1 \\\\ \\end{cases}`"
msgstr ""

